# Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_ALLOWED_USERS=123456789,987654321

# Provider API Keys (comma-separated for round-robin)
GEMINI_API_KEYS=key1,key2,key3
GROQ_API_KEYS=key1,key2
MISTRAL_API_KEYS=key1

# Default provider: gemini, groq, or mistral
DEFAULT_PROVIDER=gemini

# Agent settings
MAX_AGENT_TURNS=10
MAX_QUEUE_DEPTH=3

# Google OAuth (for Gmail + Sheets tools, optional)
GMAIL_CLIENT_ID=
GMAIL_CLIENT_SECRET=
GMAIL_REFRESH_TOKEN=

# Logging
RUST_LOG=info
